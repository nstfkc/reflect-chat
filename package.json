{
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev --concurrency 15",
    "open:ios": "dotenv -- turbo run open:ios",
    "lint": "turbo run lint",
    "db:generate": "dotenv -- turbo run db:generate",
    "db:push": "dotenv -- turbo run db:push",
    "db:studio": "dotenv -- turbo run db:studio",
    "static": "dotenv -- turbo run static",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preview": "dotenv -- turbo run preview",
    "deploy": "dotenv -- turbo run deploy",
    "postinstall": "pnpm run db:generate",
    "deploy:api": "fly deploy",
    "pack": "dotenv -- turbo run pack",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "tsup": "6.7.0",
    "turbo": "^1.9.9",
    "typescript": "5.0.4",
    "@babel/core": "^7.20.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "expo-modules-*",
        "typescript"
      ]
    }
  },
  "packageManager": "pnpm@8.5.1",
  "name": "reflect-chat",
  "dependencies": {
    "date-fns": "^2.30.0",
    "expo": "~48.0.18",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.8"
  },
  "version": "1.0.0"
}
