{
  "name": "desktop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "tsup src --watch --onSuccess \"electron dist/main.js\"",
    "bundle": "webpack-cli",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "electron ./dist/main.js",
    "clean": "rm -fr dist"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.38.0",
    "eslint-config-custom": "workspace:*",
    "ts-loader": "^9.3.0",
    "tsup": "^6.2.3",
    "typescript": "^5.0.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "electron": "^24.4.0"
  }
}
