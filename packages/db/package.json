{
  "name": "db",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./src/index.tsx",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs && tsc --emitDeclarationOnly --declaration",
    "dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "lint": "eslint \"**/*.ts*\"",
    "generate": "prisma generate",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "node seed.js"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/node": "^17.0.12",
    "swr": "^2.1.5",
    "uniqolor": "^1.1.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prisma": "^4.16.2",
    "tsconfig": "workspace:*"
  }
}
